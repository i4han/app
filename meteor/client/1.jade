template(name='logo')
    li
        #navbar-logo-i0 Getber

template(name='layout')
    +navbar
    #wrapper
        +sidebar
        +yield

template(name='layout_naked')
    +yield

template(name='logo_main')
    .logo#main-logo_main-left0 get
    .logo#main-logo_main-right0 ber

template(name='copy_main')
    #main-copy_main-hero0
        #main-copy_main-copy0 Safeguard your family rides
        .copy#main-copy_main-1 For parents
        .copy#main-copy_main-2
            +button(label="Connect with UBER" id="main-copy_main-connect0")
