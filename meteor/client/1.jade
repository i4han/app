        .copy#main-copy_main-3 For 18 years or younger
        .copy#main-copy_main-4
            +button(label="Sign up for Teens" id="main-copy_main-signup0")

template(name='main')
    #window-main-bg0
    #window-main-screen0
    #window-main-video0
    +logo_main
    +copy_main

template(name='home')
    #contentWrapper
        h2#title Sign up with UBER
        +button(label="Connect with UBER")
        .col-md-6#e3 See you soon{{hello}}
        #items
            .col-md-11#pack
                each items
                    +item

template(name='sidebar_home')
    each items
        +menu_list

template(name='world')
    h1 {{a}} world

template(name='profile')
    #contentWrapper
        with user
            | {{name}}
            | {{phone}}

template(name='item')
    .item(style='height:{{height}}px;color:{{color}}')

template(name='submit')
    h2 Connected
    +button(label="Add rider")
